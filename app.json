{
  "expo": {
    "name": "modulamp",
    "slug": "modulamp",
    "version": "1.0.0",
    "backgroundColor": "#FEFEF0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEFEF0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FEFEF0"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
